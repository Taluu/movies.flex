{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "behat/behat": "^3.3",
        "doctrine/data-fixtures": "^1.2",
        "php-http/curl-client": "^1.7",
        "php-http/message": "^1.6",
        "sensio/framework-extra-bundle": "^3.0",
        "symfony/annotations-pack": "^1.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/property-access": "^3.3",
        "symfony/serializer": "^3.3",
        "symfony/web-server-bundle": "^3.3",
        "symfony/yaml": "^3.3",
        "taluu/behapi": "^0.3.0",
        "zendframework/zend-diactoros": "^1.4"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.0@beta",
        "nelmio/alice": "^3.0@beta",
        "phpunit/phpunit": "^6.3",
        "psr/log": "^1.0",
        "symfony/dotenv": "^3.3",
        "symfony/profiler-pack": "^1.0",
        "theofidry/alice-data-fixtures": "^1.0@beta"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Behat\\": "features/context/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BPEEVHSJPZXQCQ2AJPPEKZJ8",
            "allow-contrib": "true"
        }
    }
}
